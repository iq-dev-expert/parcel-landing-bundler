{
  "name": "parcel-landing-bundler",
  "targets": {
    "default": {
      "publicUrl": "./"
    }
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "dev": "parcel src/pages/*.html -p 3000",
    "build": "parcel build src/pages/*.html",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "devDependencies": {
    "gh-pages": "^6.2.0",
    "parcel": "^2.12.0",
    "posthtml-include": "^2.0.1",
    "prettier": "3.3.3"
  }
}
